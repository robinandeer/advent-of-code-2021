        {
            "language": "typescript",
            "source": [
                "interface Position {\n  x: number;\n  depth: number;\n  aim: number;\n}\n\nconst result2 = commands.reduce<Position>((acc, command) => {\n  const [direction, steps] = command.split(\" \");\n  const stepsInt = parseInt(steps);\n\n  if (direction === \"forward\") {\n    acc.x += stepsInt;\n    acc.depth += acc.aim * stepsInt;\n  } else if (direction === \"down\") {\n    acc.aim += stepsInt;\n  } else if (direction === \"up\") {\n    acc.aim -= stepsInt;\n  }\n\n  return acc;\n}, { x: 0, depth: 0, aim: 0 });"
