            "source": [
                "interface Result {\n  forward?: number;\n  down?: number;\n  up?: number;\n}\n\nconst result = commands.reduce<Result>((acc, command) => {\n  const [direction, steps] = command.split(\" \");\n  acc[direction] = (acc[direction] || 0) + parseInt(steps);\n  return acc;\n}, {});"
